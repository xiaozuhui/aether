// 测试精确计算（分数运算）

PRINTLN("测试分数转换")
Set A TO_FRACTION(0.5)
PRINTLN(A)

Set B TO_FRACTION(0.25)
PRINTLN(B)

PRINTLN("测试分数加法")
Set SUM FRAC_ADD(A, B)
PRINTLN(SUM)

PRINTLN("测试分数转浮点")
Set RESULT TO_FLOAT(SUM)
PRINTLN(RESULT)

PRINTLN("测试GCD")
Set G GCD(12, 18)
PRINTLN(G)

PRINTLN("测试带精度计算")
Set ROUNDED ROUND_TO(3.14159, 2)
PRINTLN(ROUNDED)

Set ADD_RESULT ADD_WITH_PRECISION(0.1, 0.2, 2)
PRINTLN(ADD_RESULT)

PRINTLN("完成!")
